<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC 
		"-//mybatis.org//DTD Mapper 3.0//EN" 
		"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
		
<mapper namespace="com.itbank.tour_option.Tour_OptionDAO">
	
	<select resultType="tour_option" id="getTour_OptionList">
		select * from tour_option
	</select>
	
	<select resultType="tour_option" parameterType="int" id="selectOne">
		select * from tour_option where idx = #{one}
	</select>
	
</mapper>